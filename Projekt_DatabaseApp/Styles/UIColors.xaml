<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Backgroud Colors-->
    <Color x:Key="BgColor1">#080038</Color>
    <Color x:Key="BgColor2">#0b004d</Color>
    <Color x:Key="ABgColor1">#1f013d</Color>
    <Color x:Key="ABgColor2">#28014f</Color>
    
    <!--Border Colors-->
    <Color x:Key="winColor1">#810587</Color>
    <Color x:Key="winColor2">White</Color>
    
    <!--Text-->
    <SolidColorBrush x:Key="LogoText" Color="#9e07a6"/>
    <SolidColorBrush x:Key="unmarkedButton" Color="#525252"/>
    <SolidColorBrush x:Key="markedButton" Color="#757575"/>
    <SolidColorBrush x:Key="activeButton" Color="#33085e"/>
    
    <!--Menu Buttons-->
    <SolidColorBrush x:Key="c1" Color="White"/>
    <SolidColorBrush x:Key="c2" Color="#ba0404"/>
    <SolidColorBrush x:Key="c3" Color="#092beb"/>
    <SolidColorBrush x:Key="c4" Color="Gray"/>
    <SolidColorBrush x:Key="c5" Color="#28014f"/>
    <!--#1aad10-->

</ResourceDictionary> 